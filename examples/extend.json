// Extend layout based on DreymaR's Extend mappings
// https://forum.colemak.com/topic/2014-extend-extra-extreme/
{
  "filter": "no klc,keylayout",
  "singletonKeys": [
    [ "CapsLock", "Extend" ]
  ],
  "shiftlevels": [ "Extend", "Shift+Extend" ],
  "keys": [
    { "pos": "Escape", "letters": [ "CapsLock" ] },
    { "pos": "F1", "letters": [ "PlayPause", "AudioPause" ] },
    { "pos": "F2", "letters": [ "Previous", "AudioRewind" ] },
    { "pos": "F3", "letters": [ "Next", "AudioForward" ] },
    { "pos": "F4", "letters": [ "Stop", "Eject" ] },
    { "pos": "F5", "letters": [ "Mute", "Browser_Refresh" ] },
    { "pos": "F6", "letters": [ "VolumeDown", "BrightnessDown" ] },
    { "pos": "F7", "letters": [ "VolumeUp", "BrightnessUp" ] },
    { "pos": "F8", "letters": [ "MediaPlayer", "Sleep" ] },
    { "pos": "F9", "letters": [ "Browser", "WWW" ] },
    { "pos": "F10", "letters": [ "Search", "Mail" ] },
    { "pos": "F11", "letters": [ "Explorer", "Launch1" ] },
    { "pos": "F12", "letters": [ "Calculator", "Launch2" ] },
    { "pos": "~", "letters": [ "MouseEnable" ] },
    { "pos": "1", "letters": [ "F1" ] },
    { "pos": "2", "letters": [ "F2" ] },
    { "pos": "3", "letters": [ "F3" ] },
    { "pos": "4", "letters": [ "F4" ] },
    { "pos": "5", "letters": [ "F5" ] },
    { "pos": "6", "letters": [ "F6" ] },
    { "pos": "7", "letters": [ "F7" ] },
    { "pos": "8", "letters": [ "F8" ] },
    { "pos": "9", "letters": [ "F9" ] },
    { "pos": "0", "letters": [ "F10" ] },
    { "pos": "-", "letters": [ "F11" ] },
    { "pos": "+", "letters": [ "F12" ] },
    { "pos": "Q", "letters": [ "Esc" ] },
    { "pos": "W", "letters": [ "WheelUp", "WheelRight" ] },
    { "pos": "E", "letters": [ "Browser_Back" ] },
    { "pos": "R", "letters": [ "Browser_Forward" ] },
    { "pos": "T", "letters": [ "MouseUp" ] },
    { "pos": "Y", "letters": [ "PageUp" ] },
    { "pos": "U", "letters": [ "Home" ] },
    { "pos": "I", "letters": [ "Up" ] },
    { "pos": "O", "letters": [ "End" ] },
    { "pos": "P", "letters": [ "Delete" ] },
    { "pos": "[", "letters": [ "Esc" ] },
    { "pos": "]", "letters": [ "Insert" ] },
    { "pos": "\\", "letters": [ "Browser_Favorites", "MyComputer" ] },
    { "pos": "A", "letters": [ "Alt" ] },
    { "pos": "S", "letters": [ "WheelDown", "WheelLeft" ] },
    { "pos": "D", "letters": [ "Shift" ] },
    { "pos": "F", "letters": [ "Control" ] },
    { "pos": "G", "letters": [ "MouseDown" ] },
    { "pos": "H", "letters": [ "PageDown" ] },
    { "pos": "J", "letters": [ "Left" ] },
    { "pos": "K", "letters": [ "Down" ] },
    { "pos": "L", "letters": [ "Right" ] },
    { "pos": ";", "letters": [ "Backspace" ] },
    { "pos": "'", "letters": [ "Menu" ] },
    { "pos": "Z", "letters": [ "Undo" ] },
    { "pos": "X", "letters": [ "Cut" ] },
    { "pos": "C", "letters": [ "Copy" ] },
    { "pos": "V", "letters": [ "Paste" ] },
    { "pos": "B", "letters": [ "Button_L" ] },
    { "pos": "N", "letters": [ "Button_M" ] },
    { "pos": "M", "letters": [ "Button_R" ] },
    { "pos": ",", "letters": [ "MouseLeft" ] },
    { "pos": ".", "letters": [ "MouseRight" ] },
    { "pos": "/", "letters": [ "compose" ] },
    { "pos": "Space", "letters": [ "Enter" ] }
  ]
}
