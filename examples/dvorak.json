// Modern Dvorak Simplified Keyboard (US layout)
// https://en.wikipedia.org/wiki/Dvorak_Simplified_Keyboard
{
  "shiftlevels": [ "None", "Shift" ],
  "keys": [
    { "pos": "~", "letters": [ "`", "~" ] },
    { "pos": "1", "letters": [ "1", "!" ] },
    { "pos": "2", "letters": [ "2", "@" ] },
    { "pos": "3", "letters": [ "3", "#" ] },
    { "pos": "4", "letters": [ "4", "$" ] },
    { "pos": "5", "letters": [ "5", "%" ] },
    { "pos": "6", "letters": [ "6", "^" ] },
    { "pos": "7", "letters": [ "7", "&" ] },
    { "pos": "8", "letters": [ "8", "*" ] },
    { "pos": "9", "letters": [ "9", "(" ] },
    { "pos": "0", "letters": [ "0", ")" ] },
    { "pos": "-", "letters": [ "[", "{" ] },
    { "pos": "+", "letters": [ "]", "}" ] },
    { "pos": "Q", "letters": [ "'", "\"" ] },
    { "pos": "W", "letters": [ ",", "<" ] },
    { "pos": "E", "letters": [ ".", ">" ] },
    { "pos": "R", "letters": [ "p", "P" ] },
    { "pos": "T", "letters": [ "y", "Y" ] },
    { "pos": "Y", "letters": [ "f", "F" ] },
    { "pos": "U", "letters": [ "g", "G" ] },
    { "pos": "I", "letters": [ "c", "C" ] },
    { "pos": "O", "letters": [ "r", "R" ] },
    { "pos": "P", "letters": [ "l", "L" ] },
    { "pos": "[", "letters": [ "/", "?" ] },
    { "pos": "]", "letters": [ "=", "+" ] },
    { "pos": "\\", "letters": [ "\\", "|" ] },
    { "pos": "A", "letters": [ "a", "A" ] },
    { "pos": "S", "letters": [ "o", "O" ] },
    { "pos": "D", "letters": [ "e", "E" ] },
    { "pos": "F", "letters": [ "u", "U" ] },
    { "pos": "G", "letters": [ "i", "I" ] },
    { "pos": "H", "letters": [ "d", "D" ] },
    { "pos": "J", "letters": [ "h", "H" ] },
    { "pos": "K", "letters": [ "t", "T" ] },
    { "pos": "L", "letters": [ "n", "N" ] },
    { "pos": ";", "letters": [ "s", "S" ] },
    { "pos": "'", "letters": [ "-", "_" ] },
    { "pos": "Z", "letters": [ ";", ":" ] },
    { "pos": "X", "letters": [ "q", "Q" ] },
    { "pos": "C", "letters": [ "j", "J" ] },
    { "pos": "V", "letters": [ "k", "K" ] },
    { "pos": "B", "letters": [ "x", "X" ] },
    { "pos": "N", "letters": [ "b", "B" ] },
    { "pos": "M", "letters": [ "m", "M" ] },
    { "pos": ",", "letters": [ "w", "W" ] },
    { "pos": ".", "letters": [ "v", "V" ] },
    { "pos": "/", "letters": [ "z", "Z" ] }
  ]
}
