// AZERTY layout used in France
// https://en.wikipedia.org/wiki/Azerty
{
  "shiftlevels": [ "None", "Shift", "AltGr" ],
  "keys": [
    { "pos": "~", "letters": [ "²" ] },
    { "pos": "1", "letters": [ "&", "1" ] },
    { "pos": "2", "letters": [ "é", "2", "~" ] },
    { "pos": "3", "letters": [ "\"", "3", "#" ] },
    { "pos": "4", "letters": [ "'", "4", "{" ] },
    { "pos": "5", "letters": [ "(", "5", "[" ] },
    { "pos": "6", "letters": [ "-", "6", "¦" ] },
    { "pos": "7", "letters": [ "è", "7", "`" ] },
    { "pos": "8", "letters": [ "_", "8", "\\" ] },
    { "pos": "9", "letters": [ "ç", "9", "^" ] },
    { "pos": "0", "letters": [ "à", "0", "@" ] },
    { "pos": "-", "letters": [ ")", "°", "]" ] },
    { "pos": "+", "letters": [ "=", "+", "}" ] },
    { "pos": "Q", "letters": [ "a", "A" ] },
    { "pos": "W", "letters": [ "z", "Z" ] },
    { "pos": "E", "letters": [ "e", "E", "€" ] },
    { "pos": "R", "letters": [ "r", "R" ] },
    { "pos": "T", "letters": [ "t", "T" ] },
    { "pos": "Y", "letters": [ "y", "Y" ] },
    { "pos": "U", "letters": [ "u", "U" ] },
    { "pos": "I", "letters": [ "i", "I" ] },
    { "pos": "O", "letters": [ "o", "O" ] },
    { "pos": "P", "letters": [ "p", "P" ] },
    { "pos": "[", "letters": [ "circumflex", "diaeresis" ] },
    { "pos": "]", "letters": [ "$", "£", "¤" ] },
    { "pos": "\\", "letters": [ "*", "µ" ] },
    { "pos": "A", "letters": [ "q", "Q" ] },
    { "pos": "S", "letters": [ "s", "S" ] },
    { "pos": "D", "letters": [ "d", "D" ] },
    { "pos": "F", "letters": [ "f", "F" ] },
    { "pos": "G", "letters": [ "g", "G" ] },
    { "pos": "H", "letters": [ "h", "H" ] },
    { "pos": "J", "letters": [ "j", "J" ] },
    { "pos": "K", "letters": [ "k", "K" ] },
    { "pos": "L", "letters": [ "l", "L" ] },
    { "pos": ";", "letters": [ "m", "M" ] },
    { "pos": "'", "letters": [ "ù", "%" ] },
    { "pos": "Iso", "letters": [ "<", ">" ] },
    { "pos": "Z", "letters": [ "w", "W" ] },
    { "pos": "X", "letters": [ "x", "X" ] },
    { "pos": "C", "letters": [ "c", "C" ] },
    { "pos": "V", "letters": [ "v", "V" ] },
    { "pos": "B", "letters": [ "b", "B" ] },
    { "pos": "N", "letters": [ "n", "N" ] },
    { "pos": "M", "letters": [ ",", "?" ] },
    { "pos": ",", "letters": [ ";", "." ] },
    { "pos": ".", "letters": [ ":", "/" ] },
    { "pos": "/", "letters": [ "!", "§" ] }
  ]
}
