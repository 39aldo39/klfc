    "customDeadKeys": [
        {
            "name": "special",
            "stringMap": [
                [ "c", "©" ],
                [ "r", "®" ],
                [ "t", "™" ],
                [ "d", "°" ],
                [ "m", "µ" ],
                [ "n", "№" ],
                [ "p", "¶" ],
                [ "s", "§" ],
                [ ".", "…" ],
                [ "+", "±" ],
                [ "=", "≠" ],
                [ "<", "≤" ],
                [ ">", "≥" ],
                [ "~", "≈" ],
                [ "%", "‰" ],
                [ "|", "¦" ]
            ]
        }
    ]
