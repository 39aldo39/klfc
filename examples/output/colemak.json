                { "pos": "]", "letters": [ "]", "}", "»", "›", "Insert" ] },
                { "pos": "\\", "letters": [ "\\", "|", "cdk:special", "", "Browser_Favorites", "MyComputer" ] },
                { "pos": "A", "letters": [ "a", "A", "á", "Á", "Alt" ] },
                { "pos": "S", "letters": [ "r", "R", "grave", "", "WheelDown", "WheelLeft" ] },
                { "pos": "D", "letters": [ "s", "S", "ß", "", "Shift" ] },
                { "pos": "F", "letters": [ "t", "T", "acute", "doubleacute", "Control" ] },
                { "pos": "G", "letters": [ "d", "D", "diaeresis", "", "MouseDown" ] },
                { "pos": "H", "letters": [ "h", "H", "caron", "", "PageDown" ] },
                { "pos": "J", "letters": [ "n", "N", "ñ", "Ñ", "Left" ] },
                { "pos": "K", "letters": [ "e", "E", "é", "É", "Down" ] },
                { "pos": "L", "letters": [ "i", "I", "í", "Í", "Right" ] },
                { "pos": ";", "letters": [ "o", "O", "ó", "Ó", "Backspace" ] },
                { "pos": "'", "letters": [ "'", "\"", "õ", "Õ", "Menu" ] },
                { "pos": "Iso", "shortcutPos": "Iso", "letters": [ "-", "_", "", "", "MouseEnable" ] },
                { "pos": "Z", "letters": [ "z", "Z", "æ", "Æ", "Undo" ] },
                { "pos": "X", "letters": [ "x", "X", "circumflex", "", "Cut" ] },
                { "pos": "C", "letters": [ "c", "C", "ç", "Ç", "Copy" ] },
                { "pos": "V", "letters": [ "v", "V", "œ", "Œ", "Paste" ] },
                { "pos": "B", "letters": [ "b", "B", "breve", "", "Button_L" ] },
                { "pos": "N", "letters": [ "k", "K", "abovering", "", "Button_M" ] },
                { "pos": "M", "letters": [ "m", "M", "macron", "", "Button_R" ] },
                { "pos": ",", "letters": [ ",", "<", "cedilla", "", "MouseLeft" ] },
                { "pos": ".", "letters": [ ".", ">", "abovedot", "", "MouseRight" ] },
                { "pos": "/", "letters": [ "/", "?", "¿", "", "Compose" ] }
