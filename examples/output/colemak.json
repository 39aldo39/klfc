                { "pos": "~", "letters": [ "`", "~", "tilde", "", "MouseEnable" ] },
                { "pos": "1", "letters": [ "1", "!", "¡", "¹", "F1" ] },
                { "pos": "2", "letters": [ "2", "@", "º", "²", "F2" ] },
                { "pos": "3", "letters": [ "3", "#", "ª", "³", "F3" ] },
                { "pos": "4", "letters": [ "4", "$", "¢", "£", "F4" ] },
                { "pos": "5", "letters": [ "5", "%", "€", "¥", "F5" ] },
                { "pos": "6", "letters": [ "6", "^", "ħ", "Ħ", "F6" ] },
                { "pos": "7", "letters": [ "7", "&", "ð", "Ð", "F7" ] },
                { "pos": "8", "letters": [ "8", "*", "þ", "Þ", "F8" ] },
                { "pos": "9", "letters": [ "9", "(", "‘", "“", "F9" ] },
                { "pos": "0", "letters": [ "0", ")", "’", "”", "F10" ] },
                { "pos": "-", "letters": [ "-", "_", "–", "—", "F11" ] },
                { "pos": "+", "letters": [ "=", "+", "×", "÷", "F12" ] },
                { "pos": "Q", "letters": [ "q", "Q", "ä", "Ä", "Esc" ] },
                { "pos": "W", "letters": [ "w", "W", "å", "Å", "WheelUp", "WheelRight" ] },
                { "pos": "E", "letters": [ "f", "F", "ã", "Ã", "Browser_Back" ] },
                { "pos": "R", "letters": [ "p", "P", "ø", "Ø", "Browser_Forward" ] },
                { "pos": "T", "letters": [ "g", "G", "ogonek", "", "MouseUp" ] },
                { "pos": "Y", "letters": [ "j", "J", "đ", "Đ", "PageUp" ] },
                { "pos": "U", "letters": [ "l", "L", "ł", "Ł", "Home" ] },
                { "pos": "I", "letters": [ "u", "U", "ú", "Ú", "Up" ] },
                { "pos": "O", "letters": [ "y", "Y", "ü", "Ü", "End" ] },
                { "pos": "P", "letters": [ ";", ":", "ö", "Ö", "Delete" ] },
