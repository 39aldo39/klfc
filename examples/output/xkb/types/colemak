        level_name[Level4] = "ShiftAltGr";
        level_name[Level5] = "Extend";
        level_name[Level6] = "ShiftExtend";
        level_name[Level7] = "AltGrExtend";
        level_name[Level8] = "ShiftAltGrExtend";
    };
    
    type "NONE_EXTEND_SHIFT+EXTEND_ALTGR+EXTEND_SHIFT+ALTGR+EXTEND" {
        modifiers = LevelFive+LevelThree+Shift;
        
        map[None] = Level1;
        map[Shift] = Level1;
        map[LevelThree] = Level1;
